<resources>

    <!-- Base application theme. -->
    <style name="battery_style" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimary</item>
    </style>

    <style name="charger_style" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/material_red</item>
        <item name="colorPrimaryDark">@color/material_red</item>
        <item name="colorAccent">@color/material_red</item>
    </style>

    <style name="battery_no_title_bar_style" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimary</item>
    </style>

    <style name="battery_style_alert_dialog" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="android:textColorPrimary">@color/colorPrimary</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="charger_style_alert_dialog" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/material_red</item>
        <item name="android:textColorPrimary">@color/material_red</item>
        <item name="android:background">@color/white</item>
    </style>

    <!-- About -->
    <style name="about_name">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">@color/material_blue</item>
        <item name="android:paddingTop">10dp</item>
    </style>

    <style name="about_version">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/material_blue</item>
    </style>

    <!-- Benchmarks -->
    <style name="benchmark_realtime_container">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>

    <style name="benchmark_button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">10dp</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:paddingBottom">@dimen/activity_vertical_margin</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>

    <style name="benchmark_progress_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="benchmark_progress_error">
        <item name="android:layout_width">300dp</item>
        <item name="android:paddingTop">30dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
        <item name="android:gravity">center</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="benchmark_progress_bar" parent="android:style/Widget.ProgressBar.Horizontal">
        <item name="android:layout_width">300dp</item>
        <item name="android:layout_height">15dp</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:progress">0</item>
        <item name="android:max">100</item>
        <item name="android:progressDrawable">@drawable/progress_battery</item>
    </style>

    <!-- Brightness Benchmark -->
    <style name="benchmark_brightness_text_view">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/material_text</item>
        <item name="android:textSize">16sp</item>
    </style>

    <!-- Cpu Benchmark -->
    <style name="benchmark_cpu_text_view">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/material_text</item>
        <item name="android:textSize">16sp</item>
    </style>

    <!-- Common -->
    <style name="activity">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:paddingLeft">@dimen/activity_horizontal_margin</item>
        <item name="android:paddingRight">@dimen/activity_horizontal_margin</item>
        <item name="android:paddingTop">@dimen/activity_vertical_margin</item>
    </style>

    <style name="divider_horizontal">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/material_blue</item>
    </style>

    <style name="divider_horizontal_gray">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/light_gray</item>
    </style>

    <style name="image_button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">5dp</item>
        <item name="android:background">@color/button_material</item>
    </style>

    <style name="help_text">
        <item name="android:layout_width">350dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/material_text</item>
        <item name="android:padding">15dp</item>
    </style>

    <!-- Gadgets -->
    <style name="gadget_container">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="gadget_screen_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
    </style>

    <style name="gadget_power">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/material_blue</item>
        <item name="android:textSize">20sp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>

    <style name="gadget_slider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingBottom">15dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
    </style>

    <style name="gadget_hint">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textSize">16sp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
    </style>

    <style name="cpu_gadget_container">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">300dp</item>
    </style>

    <!-- BatteryMentorActivity -->
    <style name="powerbench_battery_life_container">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">100dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:background">@color/material_blue</item>
    </style>

    <style name="powerbench_battery_life_remaining">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">30sp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingBottom">5dp</item>
    </style>

    <style name="powerbench_battery_life_remaining_label">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="powerbench_battery_status_container">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">11dp</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="powerbench_battery_level">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="powerbench_battery_status">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="powerbench_pager_tab_container">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="powerbench_pager_tab">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">@color/tab_material_blue</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/tab_material_blue</item>
    </style>

    <style name="powerbench_button" parent="Widget.AppCompat.Button.Colored">
        <item name="android:layout_width">300dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:background">@drawable/button_material_blue</item>
    </style>

    <style name="powerbench_realtime_container">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="powerbench_power_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">22dp</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:textColor">@color/dark_gray</item>
    </style>

    <style name="powerbench_power_value_container">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
    </style>

    <style name="powerbench_power_value">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">22sp</item>
    </style>

    <style name="powerbench_power_indicator">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">3dp</item>
        <item name="android:paddingRight">3dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="powerbench_power_value_minor">
        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="powerbench_power_label">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/dark_gray</item>
        <item name="android:paddingBottom">5dp</item>
    </style>

    <style name="powerbench_power_hint">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:gravity">center|top</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/dark_gray</item>
    </style>

    <style name="powerbench_power_histogram">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="powerbench_power_histogram_axis_label">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/dark_gray</item>
    </style>

    <style name="powerbench_power_histogram_axis_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/dark_gray</item>
    </style>

    <style name="powerbench_power_item_container">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="powerbench_power_item_header">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/material_blue</item>
    </style>

    <style name="powerbench_power_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/material_blue</item>
    </style>

    <style name="powerbench_list_label_container">
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">right</item>
        <item name="android:paddingRight">15dp</item>
    </style>

    <style name="powerbench_list_label">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="powerbench_list_value_container">
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="powerbench_list_value">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="powerbench_ad">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginTop">15dp</item>
    </style>

    <!-- Process -->
    <style name="process_container">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:padding">10dp</item>
        <item name="android:layout_centerInParent">true</item>
    </style>

    <style name="process_item">
        <item name="android:layout_width">64dp</item>
        <item name="android:layout_height">64dp</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="process_name">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:text">@color/material_text</item>
    </style>

    <style name="process_cpu">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:text">@color/material_text</item>
    </style>

    <style name="process_power">
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:text">@color/material_text</item>
    </style>

    <style name="process_button_stop">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:text">@color/material_text</item>
    </style>

    <!-- Navigation -->
    <style name="navigation_button" parent="Widget.AppCompat.Button.Borderless.Colored">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:drawablePadding">10dp</item>
    </style>

    <style name="navigation_button_value" parent="navigation_button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>

    <style name="navigation_switch">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:background">@color/button_material</item>
        <item name="android:padding">10dp</item>
        <item name="android:drawablePadding">10dp</item>
    </style>

    <style name="navigation_button_container">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:background">@color/button_material</item>
    </style>

    <!-- Tips -->
    <style name="battery_tip_container">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">@dimen/activity_horizontal_margin</item>
        <item name="android:paddingRight">@dimen/activity_horizontal_margin</item>
        <item name="android:paddingTop">@dimen/activity_vertical_margin</item>
        <item name="android:paddingBottom">@dimen/activity_vertical_margin</item>
        <item name="android:background">?attr/selectableItemBackground</item>
    </style>

    <style name="battery_tip_icon">
        <item name="android:layout_width">64dp</item>
        <item name="android:layout_height">64dp</item>
        <item name="android:padding">12dp</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="battery_tip_contents_container">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_toRightOf">@+id/battery_tip_icon</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:paddingLeft">@dimen/activity_horizontal_margin</item>
    </style>

    <style name="battery_tip_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/material_text</item>
        <item name="android:paddingBottom">5dp</item>
    </style>

    <style name="battery_tip_summary">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/material_text</item>
    </style>

    <!-- Tutorial -->
    <style name="powerbench_tutorial_button">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">10dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@color/button_material</item>
    </style>

    <style name="powerbench_tutorial_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:textSize">22sp</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="powerbench_tutorial_text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">16sp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingBottom">15dp</item>
    </style>

    <style name="powerbench_tutorial_image">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
    </style>

</resources>
